{
  "name": "@maher-xubair/utils",
  "version": "1.1.2",
  "description": "A collection of utility functions for JavaScript.",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "require": "./dist/index.cjs",
      "import": "./dist/index.mjs",
      "types": "./dist/index.d.ts"
    }
  },
  "scripts": {
    "build": "tsup src/index.ts --format cjs,esm --dts --out-dir dist",
    "dev": "tsup src/index.ts --format cjs,esm --dts --out-dir dist --watch",
    "test": "node test.js"
  },
  "keywords": [
    "utils",
    "utilities",
    "javascript",
    "typescript"
  ],
  "author": "Maher Xubair",
  "license": "MIT",
  "devDependencies": {
    "@types/node": "^22.13.10",
    "tsup": "^7.2.0",
    "typescript": "^5.8.2"
  }
}
